{
    "name": "bedfordfineartgallery.com",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "nuxt",
        "build": "nuxt build",
        "start": "nuxt start",
        "generate": "node update-image-width-height.mjs && nuxt generate --modern && for f in dist/*.htm.html; do mv -- \"$f\" \"${f%.htm.html}.htm\"; done && for f in dist/*.html.html; do mv -- \"$f\" \"${f%.html.html}.html\"; done && for f in dist/ipad/*.html.html; do mv -- \"$f\" \"${f%.html.html}.html\"; done"
    },
    "lint-staged": {
        "*.{js,vue}": "eslint --cache",
        "*.**": "prettier --check --ignore-unknown"
    },
    "dependencies": {
        "@nuxt/content": "1.9.0",
        "@nuxtjs/axios": "^5.13.6",
        "@nuxtjs/markdownit": "^2.0.0",
        "core-js": "^3.19.3",
        "lodash.debounce": "^4.0.8",
        "nuxt": "^2.15.8",
        "nuxt-interpolation": "^0.2.7",
        "remove-markdown": "^0.5.0",
        "uuid": "^8.3.2",
        "vue": "^2.6.14",
        "vue-gtag": "^1.9.1",
        "vue-lazy-youtube-video": "^2.3.0",
        "vue-picture-swipe": "^2.1.0",
        "vue-server-renderer": "^2.6.14",
        "vue-slick-carousel": "^1.0.6",
        "vue-template-compiler": "^2.6.14",
        "webpack": "^4.46.0"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.16.3",
        "@nuxt/image": "^0.6.2",
        "@nuxtjs/eslint-config": "^8.0.0",
        "@nuxtjs/eslint-module": "^3.0.2",
        "@nuxtjs/router-extras": "^1.1.1",
        "eslint": "^8.4.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-nuxt": "^3.1.0",
        "eslint-plugin-vue": "^8.2.0",
        "husky": "^7.0.4",
        "image-size": "^1.0.2",
        "lint-staged": "^12.1.2",
        "prettier": "^2.5.1"
    }
}
